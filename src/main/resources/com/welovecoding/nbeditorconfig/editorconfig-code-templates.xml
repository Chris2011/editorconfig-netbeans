<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE codetemplates PUBLIC "-//NetBeans//DTD Editor Code Templates settings 1.0//EN" "http://www.netbeans.org/dtds/EditorCodeTemplates-1_0.dtd">
<codetemplates>
  <codetemplate contexts="ec-code" abbreviation="root">
    <code><![CDATA[root = ${true}${cursor}]]></code>
    <description>root</description>
  </codetemplate>
  <codetemplate contexts="ec-code" abbreviation="ist">
    <code><![CDATA[indent_style = ${space}${cursor}]]></code>
    <description>indent_style</description>
  </codetemplate>
  <codetemplate contexts="ec-code" abbreviation="isi">
    <code><![CDATA[indent_size = ${4}${cursor}]]></code>
    <description>indent_size</description>
  </codetemplate>
  <codetemplate contexts="ec-code" abbreviation="tab">
    <code><![CDATA[tab_width = ${4}${cursor}]]></code>
    <description>tab_width</description>
  </codetemplate>
  <codetemplate contexts="ec-code" abbreviation="eol">
    <code><![CDATA[end_of_line = ${lf}${cursor}]]></code>
    <description>end_of_line</description>
  </codetemplate>
  <codetemplate contexts="ec-code" abbreviation="char">
    <code><![CDATA[charset = ${utf-8}${cursor}]]></code>
    <description>charset</description>
  </codetemplate>
  <codetemplate contexts="ec-code" abbreviation="ttw">
    <code><![CDATA[trim_trailing_whitespace = ${true}${cursor}]]></code>
    <description>trim_trailing_whitespace</description>
  </codetemplate>
  <codetemplate contexts="ec-code" abbreviation="ifn">
    <code><![CDATA[insert_final_newline = ${true}${cursor}]]></code>
    <description>insert_final_newline</description>
  </codetemplate>
  <codetemplate contexts="ec-code" abbreviation="mll">
    <code><![CDATA[max_line_length = ${120}${cursor}]]></code>
    <description>max_line_length</description>
  </codetemplate>
</codetemplates>
